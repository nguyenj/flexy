{
  "blocks": "always",
  "brackets": false,
  "colons": false,
  "colors": "always",
  "commaSpace": "always",
  "commentSpace": "always",
  "cssLiteral": "never",
  "depthLimit": 4,
  "duplicates": true,
  "efficient": "always",
  "extendPref": "@extends",
  "globalDupe": false,
  "indentPref": 2,
  "leadingZero": "always",
  "maxErrors": 2,
  "maxWarnings": 10,
  "mixed": true,
  "namingConvention": false,
  "namingConventionStrict": false,
  "none": "always",
  "noImportant": true,
  "parenSpace": "never",
  "placeholders": "always",
  "prefixVarsWithDollar": "always",
  "quotePref": "single",
  "semicolons": false,
  "sortOrder": false,
  "stackedProperties": false,
  "trailingWhitespace": "never",
  "universal": true,
  "valid": true,
  "zeroUnits": false,
  "zIndexNormalize": 10
}
